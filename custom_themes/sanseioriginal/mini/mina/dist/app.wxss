/* card */
.card {
  position: relative;
  background: #fff;
  border-radius: 6rpx;
}
.card .card-title {
  align-items: start;
}
.card .card-title .card-title-text {
  flex: 1;
  margin: 0;
  padding: 8rpx;
}
.card .card-title .card-title-media {
  margin: 0;
  padding: 8rpx;
  min-width: 128rpx;
  min-height: 128rpx;
  background-color: transparent;
  align-items: center;
  justify-content: flex-start;
}
.card .card-title .card-title-media.pull-start {
  align-items: flex-start;
}
.card .card-title .card-title-media.pull-end {
