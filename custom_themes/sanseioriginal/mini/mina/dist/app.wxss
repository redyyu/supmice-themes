/* button */
button,
.button {
  display: inline-block;
  margin: 8rpx;
  padding: 16rpx;
  min-width: 1em;
  line-height: 1;
  color: rgba(51, 51, 51, 0.54);
  fill: rgba(51, 51, 51, 0.54);
  font-size: 32rpx;
  text-align: center;
  border: 0;
  border-radius: 6rpx;
  background-color: transparent;
  box-sizing: border-box;
}
button[size="xs"],
.button[size="xs"] {
  padding: 8rpx;
  font-size: 20rpx;
}
button[size="sm"],
.button[size="sm"] {
  font-size: 24rpx;
}
button[size="md"],
.button[size="md"] {
  font-size: 32rpx;
}
button[size="lg"],
.button[size="lg"] {
  font-size: 48rpx;
}
button[size="xl"],
.button[size="xl"] {
  padding: 32rpx;
  font-size: 72rpx;
}
button[disabled],
.button[disabled],
button[disabled]:not([type]),
.button[disabled]:not([type]) {
  opacity: 0.5;
  color: rgba(51, 51, 51, 0.54);
  fill: rgba(51, 51, 51, 0.54);
  background-color: transparent;
}
button.active,
.button.active {
  color: rgba(13, 13, 13, 0.54);
  fill: rgba(13, 13, 13, 0.54);
}
button.button-hover,
.button.button-hover {
  -webkit-transition: all 0.25s ease-in;
  -moz-transition: all 0.25s ease-in;
  -o-transition: all 0.25s ease-in;
  transition: all 0.25s ease-in;
  color: rgba(51, 51, 51, 0.54);
  background-color: rgba(222, 222, 222, 0.54);
}
button.raised,
.button.raised {
  color: rgba(51, 51, 51, 0.54);
  fill: rgba(51, 51, 51, 0.54);
  background-color: #e5e5e5;
  box-shadow: none;
}
button.raised.active,
.button.raised.active {
  box-shadow: none;
  background-color: #cccccc;
}
button.raised[disabled],
.button.raised[disabled] {
  color: rgba(51, 51, 51, 0.54);
  background-color: #e5e5e5;
}
button.raised.button-hover,
.button.raised.button-hover {
  -webkit-transition: all 0.25s ease-in;
  -moz-transition: all 0.25s ease-in;
  -o-transition: all 0.25s ease-in;
  transition: all 0.25s ease-in;
  color: rgba(51, 51, 51, 0.54);
  background-color: #cccccc;
}
button[plain],
.button[plain] {
  color: rgba(51, 51, 51, 0.54);
  fill: rgba(51, 51, 51, 0.54);
  border-color: rgba(51, 51, 51, 0.54);
