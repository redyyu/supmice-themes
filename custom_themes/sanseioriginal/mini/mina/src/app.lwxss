@import "_styles/mina-ui.less";

.holder {
  min-height: 720rpx;
  & .ico {
    margin: 8rpx;
  }
  & .ico.pie {
    font-size: 120rpx;
  }
}


.splash .brand {
  position: absolute;
  bottom: 30rpx;
  left: 50%;
  .transform(translateX(-50%));
  width: 180rpx;
  height: 100rpx;
  z-index: 999;
  background-size: contain;
  background-position: center;
}

.major-carousel {
  &:before,
  &:after {
    position: absolute;
    width: 60rpx;
    height: 60rpx;
    z-index: 999;
    content: ' ';
    background-color: transparent;
    background-repeat: no-repeat;
    background-image: url('data:image/svg+xml,%3Csvg%20%20xmlns%3D%22http%3A%2F%2Fwww.w3.org%2F2000%2Fsvg%22%20width%3D%2230%22%20height%3D%2230%22%20viewBox%3D%220%200%2030%2030%22%3E%3Cpath%20d%3D%22M30%2C.0005H16.4292V0H10.2084V5.1045H6.2208V0H.0006L0%2C0V0H0l0%2C0v6.22H5.1046v3.9876H0v6.2208h0V30H1.1171V16.4292h5.104V11.325h5.1036l0%2C0v0h0l0%2C0V6.2211h5.1043V1.1171H30ZM5.1042%2C1.7549V5.1042H1.1169V1.1168H5.1042Zm0%2C12.92v.638H1.1168V11.325H5.1045v3.35Zm5.1039-7.8155v3.3493H6.2211V6.2211h3.9873Zm5.1043-5.1042v3.35H11.325V1.1168h3.9877Z%22%20fill%3D%22%23c2b197%22%2F%3E%3C%2Fsvg%3E');
  }
  &:before {
    top: 16rpx;
    left: 16rpx;
  }
  &:after {
    bottom: 16rpx;
    right: 16rpx;
    .transform(rotate(180deg));
  }
}


.prd-item-img {
  width: 100%;
  height: 260rpx;
}

.product {
  & .price {
    font-size: 1.4em;
    font-weight: bold;
  }
  & .info {
    padding-top: 12rpx;
    padding-bottom: 12rpx;
    & .p {
      margin: 0;
    }
    & .tmall {
      width: 28rpx;
      height: 28rpx;
    }
  }
}

.cart-img {
  background-color: transparent !important;
}

.tag {
  padding: 0 @space-size-xs;
  background: @color-primary !important;
  border-image-source: url('data:image/png,%89PNG%0D%0A%1A%0A%00%00%00%0DIHDR%00%00%00%15%00%00%00%15%08%06%00%00%00%A9%17%A5%96%00%00%00%19tEXtSoftware%00Adobe%20ImageReadyq%C9e%3C%00%00%03%28iTXtXML%3Acom.adobe.xmp%00%00%00%00%00%3C%3Fxpacket%20begin%3D%22%EF%BB%BF%22%20id%3D%22W5M0MpCehiHzreSzNTczkc9d%22%3F%3E%20%3Cx%3Axmpmeta%20xmlns%3Ax%3D%22adobe%3Ans%3Ameta%2F%22%20x%3Axmptk%3D%22Adobe%20XMP%20Core%205.6-c140%2079.160451%2C%202017%2F05%2F06-01%3A08%3A21%20%20%20%20%20%20%20%20%22%3E%20%3Crdf%3ARDF%20xmlns%3Ardf%3D%22http%3A%2F%2Fwww.w3.org%2F1999%2F02%2F22-rdf-syntax-ns%23%22%3E%20%3Crdf%3ADescription%20rdf%3Aabout%3D%22%22%20xmlns%3AxmpMM%3D%22http%3A%2F%2Fns.adobe.com%2Fxap%2F1.0%2Fmm%2F%22%20xmlns%3AstRef%3D%22http%3A%2F%2Fns.adobe.com%2Fxap%2F1.0%2FsType%2FResourceRef%23%22%20xmlns%3Axmp%3D%22http%3A%2F%2Fns.adobe.com%2Fxap%2F1.0%2F%22%20xmpMM%3ADocumentID%3D%22xmp.did%3AEFD1FA025FE111E88227EDA1ED98ECDD%22%20xmpMM%3AInstanceID%3D%22xmp.iid%3AEFD1FA015FE111E88227EDA1ED98ECDD%22%20xmp%3ACreatorTool%3D%22Adobe%20Photoshop%20CC%202018%20%28Macintosh%29%22%3E%20%3CxmpMM%3ADerivedFrom%20stRef%3AinstanceID%3D%22xmp.iid%3AEFD1F9FD5FE111E88227EDA1ED98ECDD%22%20stRef%3AdocumentID%3D%22xmp.did%3AEFD1F9FE5FE111E88227EDA1ED98ECDD%22%2F%3E%20%3C%2Frdf%3ADescription%3E%20%3C%2Frdf%3ARDF%3E%20%3C%2Fx%3Axmpmeta%3E%20%3C%3Fxpacket%20end%3D%22r%22%3F%3E%EF%80%92%E9%00%00%00eIDATx%DAb%FC%FF%FF%3F%C3%E1M3%FE3P%09%D8%FAe02%21s%40%98%5C%83%90%F5%C2%0D%A5%D4%B5%C8%FAY%606Q%C3%DB06%E3%A1%8D%D3%A9%16%9E%18%DE%1F5t%D4%D0QCG%94%A1%8C%A0B%1AW%11FN%09%05w%29%C8%20j%94%A703%98p%D9F%89kY%A8Q%F2%A3%EB%05%080%00%2F%881%2FUj%2F%F3%00%00%00%00IEND%AEB%60%82');
  border-image-slice: 4 fill;
  border-image-repeat: stretch;
  border-image-width: 4px;
  border-image-outset: 3px;
}

.shelf-bottomsheet {
  border-top: 1rpx solid @color-primary;
  background-color: @color-light;
  .shelf-btn {
    margin: 0;
    padding: 0;
    height: 96rpx;
    line-height: 96rpx;
    vertical-align: middle;
    border-radius: 0;
  }
  .shelf-spec {
    & .spec-thumb {
      margin: 8rpx;
      width: 120rpx;
      height: 120rpx;
      padding: 16rpx;
      box-sizing: border-box;
      border-image-slice: 10 fill;
      border-image-repeat: stretch;
      border-image-width: 10px;
      border-image-source: url('data:image/png;base64,');
      &.active {
        border-image-source: url('data:image/png;base64,');
      }
      & .spec-thumb-media {
        width: 100%;
        height: 100%;
      }
    }
    & .spec-text {
      display: inline-block;
      margin: 8rpx;
      padding: 8rpx;
      min-width: 4em;
      line-height: 1.4;
      font-size: @font-sm;
      font-weight: bold;
      text-align: center;
      border-image-slice: 4 fill;
      border-image-repeat: stretch;
      border-image-width: 4px;
      border-image-source: url('data:image/png;base64,');
      &.active {
        border-image-source: url('data:image/png;base64,');
      }
    }
  }
}

.amount-count {
  min-width: 60rpx;
  text-align: center;
}