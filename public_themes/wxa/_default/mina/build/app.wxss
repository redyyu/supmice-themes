/* text */
.text-clamp-2 {
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden !important;
  text-overflow: ellipsis !important;
  line-height: 1.4em;
  height: 2.8em;
}
.text-clamp-3 {
  display: -webkit-box;
  -webkit-line-clamp: 3;
  -webkit-box-orient: vertical;
  overflow: hidden !important;
  text-overflow: ellipsis !important;
  line-height: 1.4em;
  height: 4.2em;
}
.text-truncate,
label,
.label,
.tag {
  min-width: 0;
  overflow: hidden !important;
  white-space: nowrap !important;
  text-overflow: ellipsis !important;
}
.text-center {
  text-align: center !important;
}
.text-left {
  text-align: left !important;
}
.text-right {
  text-align: right !important;
}
.text-justify {
  text-align: justify;
  text-justify: auto;
}
.text-middle {
  vertical-align: middle;
}
.text-upper {
  text-transform: uppercase !important;
}
.text-lower {
  text-transform: lowercase !important;
}
.text-capital {
  text-transform: capitalize !important;
}
.text-strike {
  text-decoration: line-through !important;
}
.text-italic {
  font-style: italic;
}
/* reset
----------------------------------------------- */
html img,
html embed,
html iframe,
html video {
  max-width: 100%;
}
html img {
  display: block;
}
html textarea {
  resize: none;
}
html mini-top-navbar {
  display: flex;
  align-items: center;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999;
  width: 100%;
  height: 45px;
  padding: 5px 10px;
  font-size: 12px;
  text-align: left;
  color: #333;
}
html mini-top-navbar > .title {
  flex: 1 1 100%;
  max-width: 75%;
  box-sizing: border-box;
  min-width: 0;
  padding: 2px 0;
  overflow: hidden !important;
  white-space: nowrap !important;
  text-overflow: ellipsis !important;
}
html mini-top-navbar > .menu {
  display: flex;
  align-items: center;
  align-content: space-between;
  flex: 1 1 100%;
  max-width: 25%;
  height: 32px;
  background: red;
  background-color: rgba(100, 100, 100, 0.1);
  border-radius: 24px;
}
html mini-top-navbar > .menu .menu-divider {
  display: block;
  width: 1px;
  height: 60%;
  background-color: inherit;
  background-color: rgba(200, 200, 200, 0.3);
}
html mini-top-navbar icon {
  width: 20px;
  height: 20px;
  margin: 0 auto;
}
html mini-bottom-tabs {
  display: flex;
  align-items: center;
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 45px;
  padding: 5px;
}
html mini-bottom-tabs .tab-item {
  flex: 1;
  padding: 0 10px;
  text-align: center;
  font-size: 12px;
  line-height: 1.6em;
}
html mini-bottom-tabs icon {
  width: 18px;
  height: 18px;
  margin: 2px auto;
}
html mini-screen {
  position: relative;
  display: block;
  padding-top: 45px;
  height: 100%;
  line-height: 1.4;
}
html mini-screen[with-tabs] {
  padding-bottom: 45px;
}
html mini-screen mini-screen-content {
  position: relative;
  display: block;
  height: 100%;
  overflow: auto;
}
html icon {
  display: block;
  font-size: 0;
  width: 24px;
  height: 24px;
  vertical-align: middle;
}
html icon > svg {
  width: 100%;
  height: 100%;
  fill: inherit;
}
img,
image,
.img {
  display: block;
  margin: 0;
  padding: 0;
  max-width: 100%;
  box-sizing: border-box;
}
view,
input,
textarea,
form {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
}
button:after {
  display: none;
  border: 0;
}
form {
  width: 100%;
}
