@import "_styles/mina-ui.lwxss";

.holder {
  min-height: 480rpx;
  & .ico {
    margin: 8rpx;
  }
  & .ico.pie {
    font-size: 120rpx;
  }
}


.product {
  & .price {
    font-size: 1.4em;
    font-weight: bold;
  }
  & .info {
    padding-top: 12rpx;
    padding-bottom: 12rpx;
    & .p {
      margin: 0;
    }
    & .tmall {
      width: 28rpx;
      height: 28rpx;
    }
  }
}

.cart-img {
  background-color: transparent !important;
}

